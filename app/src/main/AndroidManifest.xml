<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.smart.meetall">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.smart.meetall.forgot"
            android:parentActivityName="com.smart.meetall.LoginActivity"
            android:exported="false" />
        <activity
            android:name="com.smart.meetall.history"
            android:exported="false"
            android:parentActivityName="com.smart.meetall.DashboardActivity" />
        <activity android:name="com.smart.meetall.DashboardActivity" />
        <activity
            android:name="com.smart.meetall.SignupActivity"
            android:label="New Account" />
        <activity
            android:name="com.smart.meetall.LoginActivity"
            android:label="Login" />
        <activity
            android:name="com.smart.meetall.MainActivity"
            android:exported="true"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>